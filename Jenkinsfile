pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                echo"Build"
            }
        }
        stage('Deploy on Dev'){
            steps{
                echo"Deploy on Dev"
            }
        }
        stage('Deploy on QA'){
            steps{
                echo"Deploy on QA"
            }
        }
        stage('Sanity test'){
            steps{
                echo"Sanity Test"
            }
        }
        stage('Regression test'){
            steps{
                echo"Regression Test"
            }
        }

        stage('Deploy on stage'){
            steps{
                echo"Deploy on stage"
            }
        }
        stage('sanity test on stage'){
            steps{
                echo "Stage sanity test"
            }
        }
        stage('Deploy on prod'){
            steps{
                echo"Deploy on prod"
            }
        }

    }
}